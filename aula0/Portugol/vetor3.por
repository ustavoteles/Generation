programa
{
	
	funcao inicio()
	{
		cadeia lane[5]

		para(inteiro i=0;i<=4;i++){
			escreva("Digite a posição",i,": ")
			leia(lane[i])
			limpa()
		}
		
		para(inteiro i=0; i<=4;i++){
			escreva(i+": ",lane[i],"\n")
			
		}
	}
}
